<view class="container">
  <view class="filter">
    <view class="filter-pink">
      <text>Dogs</text>
    </view>
    <view class="filter-normal">
      <text>Cats</text>
    </view>
    <view class="filter-normal">
      <text>Cats</text>
    </view>
  </view>
  
  <view class="cards">
    <view class="card-category" wx:for="{{ services }}" wx:for-item="service" bindtap="goToShow" data-id="{{ service.id }}">
      <image mode="aspectFill" class="model-photo" src="{{ images[index].image }}"/>
      <view class="h1">{{ service.name }}</view>
      <view class="h2">{{ service.title }}</view>
    </view>
  </view> 
</view>

