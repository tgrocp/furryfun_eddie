<view class="cards">
<view wx:for="{{ services }}" wx:for-item="service" class="card-category" bindtap="goToShow" data-id="{{ service.id }}">
      <image mode="aspectFill" class="model-photo" src="{{ images[index].image }}"/>
      <view class="h1">{{ service.name }}</view>
      <view class="h2">{{ service.title }}</view>
  </view>
</view>

<tabBar></tabBar>
