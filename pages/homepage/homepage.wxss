  .container {
    background-size: cover;
    background-color: rgba(234,179,171,0.8);
    height:100vh;
  }
  
  .row {
    display:flex;
    justify-content: center;
    align-items: center;
    width: 100%;
  } 

  .card-product {
    width: 153px;
    padding: 10px;
  }

  .box {
    margin-right: 20px;
    display:flex;
    justify-content: center; 
    align-items: center;
    box-shadow: 0 0 rpx rgba(0,0,0,0.2);
    border-radius: 20px;
    height:126px;
    width: 153px;
    background-color: rgba(252, 246, 245, 0.7);
  }
  
  .model-icon{
    height: 90px;
    width: 90px;
    object-fit: cover;
    align-items: center;
  }

  .h1 {
    font-size: 36px;
    font-weight: bolder;
    font-family: 'Courier New', Courier, monospace;
    color: #FCF6F5;
    text-shadow: 1px 1px 3px rgba(0,0,0,0.2);
  }

  text {
    font-size: 18px;
    font-weight: bolder;
    color: #C18B80;
  }

  .h2 {
    font-size: 16px;
    font-weight: bold;
    margin: 0;
    text-align: center;
    color: #34342B;
    padding-top: 5px;
  }

  .title {
    padding: 50px 20px;

  }
